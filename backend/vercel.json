{
  "version": 2,
  "builds":[
    {
      "src":"src/app.ts",
      "use":"@vercel/node"
    }
  ],

  "routes": [
    {
      "source": "/(.*)",  
      "dest": "src/app.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin":"*"
      }
    }
  ]

}